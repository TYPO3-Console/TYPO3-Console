{
    "name": "helhum/typo3-console",
    "type": "typo3-cms-extension",
    "description": "A reliable and powerful console for TYPO3 CMS",
    "homepage": "http://typo3.org",
    "license": ["GPL-2.0+"],
    "version": "1.0.0",

    "require": {
        "typo3/cms-core": "~6.2.6"
    },
    "replace": {
        "typo3_console": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "typo3/cms": {
            "Package": {
                "partOfMinimalUsableSystem": true
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Helhum\\Typo3Console\\": "Classes"
        }
    }
}
